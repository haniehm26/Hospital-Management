<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="586.0" prefWidth="389.0" xmlns="http://javafx.com/javafx/8.0.45" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.manager">
    <children>
        <ImageView layoutX="258.0" layoutY="14.0">
            <image>
                <Image url="@../files/1.png" />
            </image>
        </ImageView>
        <ImageView fitHeight="186.0" fitWidth="323.0" layoutX="14.0" layoutY="386.0">
            <image>
                <Image url="@../files/2.png" />
            </image>
        </ImageView>
        <Label layoutX="25.0" layoutY="43.0" prefHeight="30.0" prefWidth="142.0" text="Manager Log In" textAlignment="CENTER">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <JFXTextField fx:id="managerUsername" focusColor="#1b8989" labelFloat="true" layoutX="25.0" layoutY="177.0" promptText="username" />
        <JFXPasswordField fx:id="managerPassword" focusColor="#1b8989" labelFloat="true" layoutX="25.0" layoutY="245.0" promptText="passsword" />
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#b9b6b682" height="31.0" layoutX="34.0" layoutY="310.0" stroke="BLACK" strokeType="INSIDE" width="75.0" />
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#b9b6b682" height="31.0" layoutX="305.0" layoutY="521.0" stroke="BLACK" strokeType="INSIDE" width="65.0" />
        <JFXButton layoutX="305.0" layoutY="521.0" onAction="#managerBackOnAction" prefHeight="31.0" prefWidth="65.0" text="Back">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </JFXButton>
        <JFXButton layoutX="34.0" layoutY="310.0" onAction="#managerSubmit" prefHeight="31.0" prefWidth="75.0" text="submit" wrapText="true">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </JFXButton>
    </children>
</AnchorPane>
